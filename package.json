{
  "name": "2do-cli",
  "version": "1.1.6",
  "description": "2do inside your command line",
  "main": "./lib/2do.js",
  "scripts": {
    "test": "cd ./test && tap ./*.js",
    "test:cov": "cd ./test && tap ./*.js --cov",
    "test:snap": "cd ./test && TAP_SNAPSHOT=1 tap ./*.js",
    "lint": "eslint ./lib/*.js",
    "lint:fix": "eslint ./*/*.js --fix"
  },
  "author": "Hosein2398 <hosein2398@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hosein2398/2do-cli"
  },
  "bin": {
    "2do": "./bin/2do.js"
  },
  "keywords": [
    "todo",
    "task",
    "2do",
    "console"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "cli-table3": "^0.5.1",
    "colors": "^1.3.2",
    "figlet": "^1.2.0",
    "inquirer": "^3.2.3",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "tap": "^12.1.0"
  }
}
